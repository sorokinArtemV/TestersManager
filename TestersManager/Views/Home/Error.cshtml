@{
    ViewBag.Title = "Error";
    Layout = null;
}
<link rel="stylesheet" href="~/styles.css"/>


<div class="error-container">
    <h2 class="heading heading--dark">Error</h2>
    <h2 class="subheading subheading--red">@ViewBag.ErrorMessage</h2>
    <img src="~/error.svg" alt="error" class="error-image">
</div>